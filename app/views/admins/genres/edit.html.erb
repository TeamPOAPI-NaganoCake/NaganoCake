<div class="row">
   <h5 class="title"><span style="background-color:#C0C0C0">ジャンル編集</span></h5>
</div>
<div class="row">
  <%= form_with model:@genre, url: admins_genre_path(@genre.id), method: :patch, local:true do |f| %>
    <div>
      <%= f.label :"ジャンル名", class:"form" %>
      <%= f.text_field :genre_name, class:"form" %>
      <%= f.submit "変更を保存", class: "btn btn-success" %>
    <% end %>
  </div>
</div>
